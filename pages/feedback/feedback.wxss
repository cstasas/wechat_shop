page {
  background-color: #eee;
}
Tabs {
  background-color: #fff;
}
.feedback_main {
  font-size: 24rpx;
  color: #666;
}
.feedback_main .feedback_content .feedback_content_title {
  margin: 10rpx;
}
.feedback_main .feedback_content .feedback_content_text {
  display: flex;
  flex-wrap: wrap;
}
.feedback_main .feedback_content .feedback_content_text text {
  width: 28%;
  text-align: center;
  margin: 20rpx;
  padding: 10rpx 30rpx;
  background-color: #fff;
}
.feedback_main .feedback_textarea {
  width: 96%;
  margin-left: 2%;
  background-color: #fff;
}
.feedback_main .feedback_textarea textarea {
  width: 100%;
  padding: 10rpx;
}
.feedback_main .feedback_textarea .feedback_textarea_btn {
  display: flex;
  margin: 10rpx 0;
}
.feedback_main .feedback_textarea .feedback_textarea_btn button {
  margin: 0;
  width: 90rpx;
  height: 90rpx;
  margin-left: 20rpx;
  margin-bottom: 20rpx;
  font-size: 40rpx;
  border: 1px solid #ccc;
  color: #ccc;
  display: flex;
  justify-content: center;
  align-items: center;
}
.feedback_main .feedback_textarea .feedback_textarea_btn .UpImg {
  display: flex;
  flex-wrap: wrap;
}
.feedback_main .feedback_textarea .feedback_textarea_btn .UpImg UpImg {
  margin-left: 20rpx;
  margin-bottom: 20rpx;
}
.feedback_main .feedback_btn button {
  background-color: #da2b2b;
  width: 30%;
  margin-top: 20rpx;
  margin-right: 2%;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  font-weight: 400;
}
